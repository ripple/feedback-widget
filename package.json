{
  "name": "feedback-widget",
  "version": "1.0.0",
  "description": "A themeable and embeddable widget for receiving user feedback.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack-cli --mode production",
    "start": "webpack-dev-server",
    "test": "jest",
    "test-update-snapshots": "jest --updateSnapshot",
    "deploy": "npm run build && gh-pages -d dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o dist/storybook",
    "lint": "./node_modules/.bin/eslint ."
  },
  "babel": {
    "presets": [
      "airbnb"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js?(x)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test-helpers/"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "serve": {
    "content": [
      "./dist",
      "./public"
    ]
  },
  "author": "seriousben https://github.com/seriousben",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-decorators": "7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "7.7.4",
    "@babel/plugin-proposal-function-sent": "7.7.4",
    "@babel/plugin-proposal-json-strings": "7.7.4",
    "@babel/plugin-proposal-numeric-separator": "7.7.4",
    "@babel/plugin-proposal-throw-expressions": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-syntax-import-meta": "7.7.4",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-links": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "5.2.8",
    "@types/styled-components": "^5.1.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-airbnb": "4.4.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^3.6.0",
    "cssimportant-loader": "0.4.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-webpack": "0.12.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^2.2.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.14.1",
    "postcss-increase-specificity": "0.6.0",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.2.0",
    "typescript": "^4.2.4",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-obfuscator": "0.22.1",
    "webpack-serve": "3.2.0"
  },
  "dependencies": {
    "@analytics/google-analytics": "^0.5.2",
    "@analytics/google-tag-manager": "^0.5.0",
    "ajv": "^7.2.4",
    "analytics": "^0.7.5",
    "prop-types": "^15.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-slidy": "^4.1.0",
    "react-step-builder": "^2.0.10",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.3.0",
    "uniforms": "^3.5.0",
    "uniforms-bootstrap4": "^3.5.0",
    "uniforms-bridge-json-schema": "^3.5.0",
    "uniforms-bridge-simple-schema": "^3.5.0",
    "uniforms-bridge-simple-schema-2": "^3.5.0",
    "uniforms-semantic": "^3.5.0",
    "uniforms-unstyled": "^3.5.0"
  }
}
